<!DOCTYPE html>
<html>
	<head>

		<meta charset="utf-8" />

		<title><%- @title %></title>

		<link href="style.css" rel="stylesheet" />

		<script src="jquery/dist/jquery.min.js"></script>
		<script src="script.js"></script>

	</head>
	<body>

		<h1><%- @title %></h1>

		<% for post in @posts : %>
		
		<section class="post">
			<h2><%- post.title %></h2>

			<p><%- post.text %></p>
		</section>
		<% end %>

	</body>
</html>